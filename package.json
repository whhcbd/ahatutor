{
  "name": "ahatutor",
  "version": "0.1.0",
  "description": "AhaTutor - 遗传学可视化交互解答平台",
  "private": true,
  "workspaces": [
    "src/frontend",
    "src/backend",
    "src/shared"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd src/frontend && npm run dev",
    "dev:backend": "cd src/backend && npm run start:dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd src/frontend && npm run build",
    "build:backend": "cd src/backend && npm run build",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd src/frontend && npm run lint",
    "lint:backend": "cd src/backend && npm run lint",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd src/frontend && npm run test",
    "test:backend": "cd src/backend && npm run test"
  },
  "keywords": [
    "genetics",
    "education",
    "visualization",
    "rag",
    "llm",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.5.0"
  }
}
